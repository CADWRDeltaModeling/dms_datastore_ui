{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "repoui",
            "type": "debugpy",
            "request": "launch",
            "module": "panel",
            "console": "integratedTerminal",
            "args": [
                "serve",
                "--dev",
                "--autoreload",
                "--show",
                "${workspaceFolder}/repoui.py",
            ],
            "cwd": "y:/repo",
            "justMyCode": false,
        },
        {
            "name": "uimgr",
            "type": "debugpy",
            "request": "launch",
            "module": "panel",
            "console": "integratedTerminal",
            "args": [
                "serve",
                "--dev",
                "--autoreload",
                "--show",
                "dms_datastore_ui/datastore_uimgr.py",
            ],
            "justMyCode": false,
        },
        {
            "name": "flag editor",
            "type": "debugpy",
            "request": "launch",
            "program": "dms_datastore_ui/flag_editor.py",
            "console": "integratedTerminal",
            "args": [
                "tests/repo/continuous/screened/ncro_oad_*_temp_*.csv"
            ]
        },
        {
            "name": "data screener",
            "type": "debugpy",
            "request": "launch",
            "program": "dms_datastore_ui/data_screener.py",
            "console": "integratedTerminal",
            "args": [
                "\"tests/repo/continuous/screened/ncro_oad_*_temp_*.csv\""
            ]
        },
        {
            "name": "show-repo",
            "type": "debugpy",
            "request": "launch",
            "program": "-m",
            "args": [
                "dms_datastore_ui.cli",
                "show-repo",
                "y:/repo/continuous",
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "autoReload": {
                "enable": true,
            }
        },
        { "name": "visgap",
            "type": "debugpy",
            "request": "launch",
            "program": "notebooks/vis_gap.py",
            "console": "integratedTerminal"
        }

    ]
}